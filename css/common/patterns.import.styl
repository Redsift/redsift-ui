$pattern-down-arrow-green-height = 7px
$pattern-down-arrow-green-width = 12px
$pattern-redsift-logo-height = 7px
$pattern-redsift-logo-width = 7px

.-pattern-down-arrow-green {
  background-image: url("/icons/down-arrow-green.svg");
  background-repeat: repeat;
  background-size: $pattern-down-arrow-green-width $pattern-down-arrow-green-height;
}

.-pattern-redsift-logo {
  background-image: url("/icons/redsift-logo-red-8.1.svg");
  background-repeat: repeat;
  background-size: $pattern-redsift-logo-width $pattern-redsift-logo-height;
}
